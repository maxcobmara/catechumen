			<td style="font-size:13px;font-family: Arial, sans-serif;"><%#=count+1 %>&nbsp;</td>
			<td style="font-size:13px;font-family: Arial, sans-serif;"><%#= Student.find(student_id).matrixno %></td>
			<td colspan=2 style="font-size:13px;font-family: Arial, sans-serif;"><%#= Student.find(student_id).name %></td>
			<td style="font-size:13px;font-family: Arial, sans-serif;" width=7%>cc</td>
			<td rowspan=24 style="font-size:13px;font-family: Arial, sans-serif;"width=7%>dd</td>
			<td style="font-size:13px;font-family: Arial, sans-serif;" width=7%>e</td>
			<td rowspan=24 style="font-size:13px;font-family: Arial, sans-serif;" width=7%>f</td>
			<td style="font-size:13px;font-family: Arial, sans-serif;"width=7%>g</td>
			<td rowspan=24 style="font-size:13px;font-family: Arial, sans-serif;"width=7%>h</td>
		</tr>-->
		<!--end-if record not updated yet, weeklytimetable_details_id not INSERTED yet-->
	<%# end %><!--end for @exist_student-->
	
	<% remaining_count = @studentattendances_group.count+1 %>
	<% remaining_count.upto(24) do |index| %>
	<tr>
		<td style="font-size:13px;font-family: Arial, sans-serif;"><%=index%></td>
		<td style="font-size:13px;font-family: Arial, sans-serif;">ss</td>
		<td colspan=2 style="font-size:13px;font-family: Arial, sans-serif;">ee</td>
		<td width=8% style="font-size:13px;font-family: Arial, sans-serif;">rr</td>
		<td width=8% style="font-size:13px;font-family: Arial, sans-serif;">tt</td>
		<td width=8% style="font-size:13px;font-family: Arial, sans-serif;">yy</td>
	</tr>
	<% end %>
	
	<tr>
		<td></td>
		<td></td>
