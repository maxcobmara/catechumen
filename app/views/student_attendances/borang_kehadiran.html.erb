	<% @exist_student = @studentattendances_group.count %>
	<%# if @exist_student > 0 %>
		<% @studentattendances_group.each do |student_id, studentattendances|  %>
			<% if count < @exist_student %>
				<% studentattendances.each_with_index do |studentattendance, no|%>
					<% if no==0 %>
					<tr>
						<td style="font-size:13px;font-family: Arial, sans-serif;"><%=count+1 %>&nbsp;</td>
						<td style="font-size:13px;font-family: Arial, sans-serif;"><%= Student.find(student_id).matrixno %></td>
						<td colspan=2 style="font-size:13px;font-family: Arial, sans-serif;"><%= Student.find(student_id).name %></td>
						<td style="font-size:13px;font-family: Arial, sans-serif;" width=7%>cc</td>
						<% if no==0 && count==0 %>
							<td rowspan=24 style="font-size:13px;font-family: Arial, sans-serif;"width=7%>
								<% if @student_attendances && @student_attendances[0].weeklytimetable_details_id!= nil %>
									<%= @student_attendances[0].weeklytimetable_detail.weeklytimetable_topic.name %>
								<% else %>
									&nbsp;
								<% end %>
							</td>
						<% end%>
						<td style="font-size:13px;font-family: Arial, sans-serif;" width=7%>e</td>
						<% if no==0 && count==0 %>
							<td rowspan=24 style="font-size:13px;font-family: Arial, sans-serif;" width=7%>
								<% if @student_attendances && @student_attendances[1].weeklytimetable_details_id!= nil %>
									<%= @student_attendances[1].weeklytimetable_detail.weeklytimetable_topic.name %>
								<% else %>
									&nbsp;
								<% end %>
							</td>
						<% end %>
						<td style="font-size:13px;font-family: Arial, sans-serif;"width=7%>g</td>
						<% if no==0 && count==0 %>
							<td rowspan=24 style="font-size:13px;font-family: Arial, sans-serif;"width=7%>
								<% if @student_attendances && @student_attendances[2].weeklytimetable_details_id!= nil %>
									<%= @student_attendances[2].weeklytimetable_detail.weeklytimetable_topic.name %>
								<% else %>
									&nbsp;
								<% end %>
							</td>
						<% end %>
					</tr>
					<% end %>
				<% end %>
				<% count+=1%>
			<% end %>
		<% end %>
	<%# else%>
