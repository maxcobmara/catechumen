<div class="tform">
	<H2>Student Attendance Details</H2>
	<table width=100%>
		<tr>
			<td class="leftcol"><%= f.label :student_id %></td>
			<td><%= f.collection_select :student_id, Student.all, :id, :matrix_name %></td>
		</tr>
		<tr>
			<td class="leftcol"><%#= f.label :weeklytimetable_details_id %><label>Date | Time | Subject-Topic</label></td>
			<td><%= f.collection_select :weeklytimetable_details_id, WeeklytimetableDetail.all, :id, :day_time_slot %><%#= f.text_field :weeklytimetable_details_id %></td>
		</tr>
		<tr>
			<td class="leftcol"> <%= f.label :attend %></td>
			<td><%= f.check_box :attend %></td>
		</tr>
	</table>
</div>





  

  
  

