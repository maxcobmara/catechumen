<table width=100%>
	<tr>
		<td class="leftcol"><%= f.label :intake_name %></td>
		<td><%= f.text_field :name %>&nbsp; (Eg.: Jan 2013, March 2013, July 2013)</td>
	</tr>
	<tr>
		<td class="leftcol"><%= f.label :group %></td>
		<td><%= f.text_field :description %></td>
	</tr>
	<tr>
		<td class="leftcol"><%= f.label :register_on %></td>
		<td><%= f.date_select :register_on %></td>
	</tr>
	<tr>
		<td class="leftcol"><%= f.label :programme_id %></td><!--18March2013-->
		<td><%= f.collection_select :programme_id, Programme.roots, :id, :programme_list, :include_blank => true %></td>
	</tr>
	<tr>
		<td class="leftcol"><%= f.label :is_active %></td>
		<td><%= f.check_box :is_active %></td>
	</tr>
</table>