	<table class="plong">
		<tr><td colspan="6" class="header9ab">METHODOLOGI</td></tr>
		<tr>
			<td class="header9abc">Masa</td>
			<td class="header9abc">Isi kandungan/Sub topik</td>
			<td class="header9abc">Aktiviti Pengajar</td>
			<td class="header9abc">Aktiviti Pelatih</td>
			<td class="header9abc">Bahan & Alat Bantuan Mengajar</td>
			<td class="header9abcd">Penilaian</td>
		</tr>
		<% @lesson_plan.lessonplan_methodologies.each do |l| %>
		<tr>
			<td class="data6">
				<%= h l.start_meth.strftime("%H:%M") %> - <%= h l.end_meth.strftime("%H:%M %p") %>
				<br>(<%=h (((l.end_meth - l.start_meth )/60 ) % 60).to_i %> minutes) 
			</td>
			<td class="data6"><%= simple_format(l.content) %></td>
			<td class="data6"><%= simple_format(l.lecturer_activity) %></td>
			<td class="data6"><%= simple_format(l.student_activity) %></td>
			<td class="data6"><%= simple_format(l.training_aids) %></td>
			<td class="data7"><%= simple_format(l.evaluation) %></td>
		<tr>	
		<% end%>
		<tr><td colspan="6" style="height:30px;"></td></tr>
		<tr>
			<td></td>
			<td colspan="2" class="sign_prepare2">
