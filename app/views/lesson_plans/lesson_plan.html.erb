<H4 align="center" class="heading">PELAN PENGAJARAN 
<% if @lesson_plan.schedule_item.lecture_method == 1 %>
      KULIAH
<% elsif @lesson_plan.schedule_item.lecture_method == 2 %>
      TUTORIAL
<% elsif @lesson_plan.schedule_item.lecture_method == 3 %>
      AMALI
<% end %>
</H4>
<table class="plong">	
	<tr>
		<td class="header7">Nama Pengajar</td>
		<td class="header9a"><%=h @lesson_plan.lessonplan_owner.name %></td>
	</tr>
	<tr>
		<td class="header7">Kumpulan Pelatih</td>
		<td class="header9">Kumpulan <%=h @lesson_plan.schedule_item.weeklytimetable.schedule_intake.group_with_intake_name %>
							<%#=h @lesson_plan.lessonplan_intake.group_with_intake_name%>
		</td>
	</tr>
	<tr>
		<td class="header7">Bilangan Pelatih</td>
		<td class="header9"><%=h @lesson_plan.student_qty %></td>
	</tr>
	<tr>
		<td class="header7">Tahun<br>Semester</td>
		<td class="header9">Tahun <%=h @lesson_plan.year %><br>Semester <%=h @lesson_plan.semester %></td>
	</tr>
	<tr>
		<td class="header7">Topik / Tajuk</td>
		<td class="header9">
			<%=h @lesson_plan.schedule_item.weeklytimetable_topic.full_parent %><br>
			<%=h @lesson_plan.schedule_item.weeklytimetable_topic.name %>
			<%#=h @lesson_plan.lessonplan_topic.subject_with_topic %><!--<br>--><%#=h @lesson_plan.lecture_title %>
		</td>
	</tr>
	<tr>
		<td class="header7">Tarikh & Masa</td>
		<td class="header9">
			<%=h @lesson_plan.schedule_item.get_date_for_lesson_plan %>
			(<%=h @lesson_plan.schedule_item.get_start_time %> - <%=h @lesson_plan.schedule_item.get_end_time %>)
			
			<%#=h @lesson_plan.lecture_date %> <!--
			(<%#=h @lesson_plan.start_time.strftime("%H:%M") %> - <%#=h @lesson_plan.end_time.strftime("%H:%M") %>) 
			(<%#=@lesson_plan.end_time.to_f-@lesson_plan.start_time.to_f%>)-->
		</td>
	</tr>
	<tr>
		<td class="header7">Pengetahuan Terdahulu<br><i>(Prerequisite)</i></td>
		<td class="header9"><%=h @lesson_plan.prerequisites %></td>
	</tr>
	<tr>
		<td class="objective">Objektif Pembelajaran</td>
		<td class="objective2"><%#= @lesson_plan.lessonplan_topic.topic_details.last.objctives %>
		<%=h check_kin {simple_format(@lesson_plan.schedule_item.weeklytimetable_topic.topic_details.last.objctives)} %></td>
	</tr>
	<tr>
		<td class="header9a">Rujukan</td>
		<td class="header10"><%=h simple_format(@lesson_plan.reference) %></td>
	</tr>

	<tr><td colspan="8" style="height:10px;">&nbsp;</td><tr>
		<tr><td colspan="8" style="height:10px;">&nbsp;</td><tr>
			<tr><td colspan="8" style="height:10px;">&nbsp;</td><tr>
				<tr><td colspan="8" style="height:10px;">&nbsp;</td><tr>
					<tr><td colspan="8" style="height:10px;">&nbsp;</td><tr></table>
