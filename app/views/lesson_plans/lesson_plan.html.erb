				<%=h @lesson_plan.prepared_by? ? @lesson_plan.lessonplan_creator.position.name : '-' %><br>
				Kolej Sains Kesihatan Bersekutu Johor Bahru<br><br>
				Tarikh : <%=h  @lesson_plan.submitted_on? ? @lesson_plan.submitted_on.strftime('%d-%b-%Y') : "Not Submitted" %>
			</td>
			<td colspan="2">

			</td>
			<td colspan="3" class="sign_prepare2">
				<span class="bold-cap"><%=h @lesson_plan.endorsed_by? ? @lesson_plan.endorser.name : "-"  %></span><br>
				<%=h @lesson_plan.endorsed_by? ? @lesson_plan.endorser.position.name : "-"  %><br>
				Kolej Sains Kesihatan Bersekutu Johor Bahru<br><br>
				Tarikh : <%=h @lesson_plan.hod_approved_on? ? @lesson_plan.hod_approved_on.strftime('%d-%b-%Y') : "Not Approved" %>
			</td>
		</tr>
		<tr><td colspan="6" style="height:30px;"></td></tr>
