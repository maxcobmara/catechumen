<!-- Box -->
<div class="box">
	<!-- Box Head -->
	<div class="box-head">
	    <h2 class="left">Tenancy</h2>
		  		<div class="right">
				  <% form_tag tenants_path, :method => 'get' do %>
					<%= text_field_tag :search, params[:search] %>
					<%= submit_tag I18n.t('search') %>
					<% end %>
				</div>
		</div>
</div>

<div class="tlist">
<table width=100% border="0" cellpadding="0" cellspacing="0">
  <tr>
    <th>Location Name</th>
    <th>Staff Name</th>
    <th>Studen Name</th>
    <th>Programme</th>
    <th>Key Accept</th>
    <th>Key Expected Return</th>
    <th>Key Returned</th>
    <th>Force vacate</th>
    <th colspan=4 class="ac">Control</th>
  </tr>

<% @tenants.each do |tenant| %>
  <tr>
    <td><%=h tenant.location_name %></td> <!-- 16/11/2011 - Shaliza added code for location if no longer exist.-->
    <td><%=h check_kin {tenant.staff_name} %><%#=h tenant.staff_name %></td> <!-- 16/11/2011 - Shaliza added code for staff if no longer exist.-->
    <td><%#=h tenant.student_name %></td> <!-- 16/11/2011 - Shaliza added code for student if no longer exist.-->
    <td><%#=h tenant.course_details %></td> <!-- 16/11/2011 - Shaliza added code for course if no longer exist.-->
    <td><%=h l(tenant.keyaccept) unless tenant.keyaccept.blank? %></td>
    <td><%=h l(tenant.keyexpectedreturn) unless tenant.keyexpectedreturn.blank? %></td>
    <td><%=h l(tenant.keyreturned) unless tenant.keyreturned.blank? %></td>
    <td><%=h tenant.force_vacate? ? "Yes" : "No" %></td><!--16/11/2011 - Shaliza added code to display Yes or No not true or false-->
	  <td width=10px>	
			<% if tenant.location.typename == 2 %> 
				<%= link_to image_tag("printer.png", :border => 0, :title => 'Borang Asrama'), :action => 'borang_asrama', :id => tenant %>
		  <% end %></td>

  <td width=10px><%= link_to image_tag("document.png", :border => 0), :action => 'show', :id => tenant %></td>
	<td width=10px><%= link_to image_tag("edit.png", :border => 0), :action => 'edit', :id => tenant %></td>
	<td width=10px><%= link_to image_tag("delete.png", :border => 0, :title => 'Delete'), tenant, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
</div>
<br />

