<div id="tprintreport">	
	<table width=80% border=1px cellspacing=0 align="center">
		
		<% asal = 0.00 %><% current = 0.00 %><% jualan = 0.00 %><% pindahan = 0.00 %><% musnah = 0.00 %> <% lain = 0.00 %>
		<% @asset_disposals=AssetDisposal.find(:all, :conditions=>['id IN (?)',@assetsearch.disposalreport2.split(",") ]) %>
		<% @asset_disposals.each do |asset_disposal| %>	
			<% asal+=asset_disposal.asset.purchaseprice.to_f %>
			<% current+=asset_disposal.current_value.to_f %>
			<%# musnah += asset_disposal.current_value if asset_disposal.disposal_type == "discard" %>
			<%# jualan += asset_disposal.current_value if asset_disposal.disposal_type == "sold" %>
			<%# pindahan += asset_disposal.current_value if asset_disposal.disposal_type == "transfer" %>
			<%# lain += asset_disposal.current_value if ((asset_disposal.disposal_type != "transfer") && (asset_disposal.disposal_type != "sold") && (asset_disposal.disposal_type != "discard") )%>
			<% if asset_disposal.disposal_type == "discard" %>
				<% musnah += asset_disposal.current_value %>
			<% elsif asset_disposal.disposal_type == "sold" %>
				<% jualan += asset_disposal.current_value %>
			<% elsif asset_disposal.disposal_type == "transfer" %>
				<% pindahan += asset_disposal.current_value %>
			<% else %>
				<% if asset_disposal.current_value!= nil%>
					<% lain += asset_disposal.current_value%>
				<% else %>
					<% lain = 0.00 %>
				<% end %>
			<% end %>
		<% end %>
		
		
