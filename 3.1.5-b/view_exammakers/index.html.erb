<div class="box">
	<div class="box-head">
		<h2 class="left">List of Examination Paper</h2>
	</div>
</div>
<div class="tlist">
	<table width=90% border="0" cellpadding="0" cellspacing="0">
	  <tr>
	    <th>Name</th>
	    <th>Description</th>
		<th>Subject</th>
	    <th>Creator</th>
		<th colspan=3>Control</th>
	  </tr>

		<% @exammakers.each do |exammaker| %>
		  <tr>
		    <td><%=h exammaker.render_examtype %></td>
		    <td><%=h exammaker.description %></td>
		    <td><%=h exammaker.examination.subject_code_with_subject_name %></td>
			<td><%=h exammaker.examcreator.name %></td>
		    <td width=10px><%= link_to image_tag("document.png", :border => 0, :title => 'Show'), :action => 'show', :id => exammaker %></td>
		    <td width=10px><%= link_to image_tag("edit.png", :border => 0, :title => 'Edit'), :action => 'edit', :id => exammaker %></td>
		    <td width=10px><%= link_to image_tag("delete.png", :border => 0, :title => 'Delete'), exammaker, :confirm => 'Are you sure?', :method => :delete %></td>
		  </tr>
		<% end %>
		</table>

<br />
</div>
<%= link_to 'New Examination Paper', new_exammaker_path %>